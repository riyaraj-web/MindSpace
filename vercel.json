{
  "version": 2,
  "name": "mindspace",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/dashboard",
      "dest": "/index.html"
    },
    {
      "src": "/mood",
      "dest": "/index.html"
    },
    {
      "src": "/meditation",
      "dest": "/index.html"
    },
    {
      "src": "/journal",
      "dest": "/index.html"
    },
    {
      "src": "/breathing",
      "dest": "/index.html"
    },
    {
      "src": "/habits",
      "dest": "/index.html"
    },
    {
      "src": "/recovery-stories",
      "dest": "/index.html"
    },
    {
      "src": "/ai-assistant",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "framework": "vite"
}